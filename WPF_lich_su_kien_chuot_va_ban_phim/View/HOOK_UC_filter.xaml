<UserControl x:Class="WPF_lich_su_kien_chuot_va_ban_phim.View.HOOK_UC_filter"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPF_lich_su_kien_chuot_va_ban_phim.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        
        <Grid Grid.Row="1" Grid.Column="1">
            <Grid Background="Yellow">
                <StackPanel>
                    <TextBlock Foreground="Transparent" FontSize="30"></TextBlock>
                    <TextBlock x:Name="Setting" Foreground="#FF979DA5" FontSize="30" Margin="20,0,0,0" HorizontalAlignment="Center">Setting</TextBlock>
                </StackPanel>
                <Button Style="{StaticResource InvisibleButton}" Background="Transparent" Height="60" Width="300" Margin="20,10,0,0" BorderBrush="Transparent"></Button>
            </Grid>
        </Grid>
        
    </Grid>
</UserControl>
